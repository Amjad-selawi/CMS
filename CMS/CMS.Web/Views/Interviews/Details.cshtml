@model CMS.Application.DTOs.InterviewsDTO

<h2>Details</h2>

<div>
    <h4>Interview</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Interview ID
        </dt>
        <dd class="col-sm-10">
            @Model.InterviewsId
        </dd>
        <dt class="col-sm-2">
            Score
        </dt>
        <dd class="col-sm-10">
            @Model.Score
        </dd>
        <dt class="col-sm-2">
            Date
        </dt>
        <dd class="col-sm-10">
            @Model.Date
        </dd>
        <dt class="col-sm-2">
            Parent ID
        </dt>
        <dd class="col-sm-10">
            @Model.ParentId
        </dd>
        <dt class="col-sm-2">
            Candidate ID
        </dt>
        <dd class="col-sm-10">
            @Model.CandidateId
        </dd>
        <dt class="col-sm-2">
            Status
        </dt>
        <dd class="col-sm-10">
            @Model.Status
        </dd>
        <dt class="col-sm-2">
            Notes
        </dt>
        <dd class="col-sm-10">
            @Model.Notes
        </dd>
     
      
    </dl>
</div>
<div class="form-group">
    <a class="btn btn-secondary" asp-action="Index">Back to List</a>
    <a asp-action="Edit" asp-route-id="@Model.InterviewsId" class="btn btn-primary">Edit</a>
    
