@model CMS.Application.DTOs.Register

<h2>User Details</h2>

<div>
    <dl class="row">
        <dt class="col-sm-2">User ID</dt>
        <dd class="col-sm-10">@Model.RegisterrId</dd>
        <dt class="col-sm-2">Email</dt>
        <dd class="col-sm-10">@Model.Email</dd>
        <dt class="col-sm-2">Username</dt>
        <dd class="col-sm-10">@Model.UserName</dd>

        <dt class="col-sm-2">Role</dt>
        <dd class="col-sm-10">@Model.SelectedRole</dd>
    </dl>
</div>

<div class="form-group " style="margin-top: 10px;">
    <a asp-action="Edit" asp-route-id="@Model.RegisterrId" class="btn btn-primary">Edit</a>
    <a class="btn btn-secondary" asp-action="Index">Back to List</a>


</div>